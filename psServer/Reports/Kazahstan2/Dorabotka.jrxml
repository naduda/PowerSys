	<subDataset name="ds_Result" uuid="caba981c-9096-498f-a9cc-b598d731adbd">
		<queryString>
			<![CDATA[select times.dt, v1.val, v2.val as val2, v3.val as val3,
case getval_ts(95,times.dt) when 1 then 'Ток' when 2 then 'Напряжение' else 'Потенциал' end as val4, v5.val as val5,
case getval_ts(99,times.dt) when 1 then 'Отключен' else 'Включен' end as val6, v7.val as val7, v8.val as val8 from
(
select  to_date('2014-01-12','YYYY-DD-MM') + cast((i::varchar || ' hour') as interval) as dt
from generate_series(0,(to_date('2015-01-03','YYYY-DD-MM')-to_date('2014-01-12','YYYY-DD-MM'))*48) as i
) times
left join(select dt, val, signalref from f_arcvalti(87, '2014-01-12', '2015-01-03', 60) order by dt) as v1 on v1.dt = times.dt
left join( select dt, val, signalref from f_arcvalti(86, '2014-01-12', '2015-01-03', 60) order by dt) as v2 on v2.dt = times.dt
left join( select dt, val, signalref from f_arcvalti(89, '2014-01-12', '2015-01-03', 60) order by dt) as v3 on v3.dt = times.dt
left join( select dt, val, signalref from f_arcvalti(96, '2014-01-12', '2015-01-03', 60) order by dt) as v5 on v5.dt = times.dt
left join( select dt, val, signalref from f_arcvalti(27, '2014-01-12', '2015-01-03', 60) order by dt) as v7 on v7.dt = times.dt
left join( select dt, val, signalref from f_arcvalti(29, '2014-01-12', '2015-01-03', 60) order by dt) as v8 on v8.dt = times.dt
where v1.val is not null]]>
		</queryString>
		<field name="dt" class="java.sql.Timestamp"/>
		<field name="val" class="java.lang.Double"/>
		<field name="val2" class="java.lang.Double"/>
		<field name="val3" class="java.lang.Double"/>
		<field name="val4" class="java.lang.String"/>
		<field name="val5" class="java.lang.Double"/>
		<field name="val6" class="java.lang.String"/>
		<field name="val7" class="java.lang.Double"/>
		<field name="val8" class="java.lang.Double"/>
	</subDataset>
