<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.cell.*?> 
<?import javafx.scene.text.Text?> 

<Pane fx:id="bpAlarms" xmlns:fx="http://javafx.com/fxml" fx:controller="controllers.journals.TableController">
	<BorderPane prefWidth="${bpAlarms.width}" prefHeight="${bpAlarms.height}" >
		<padding><Insets top="0" right="0" bottom="0" left="0"/></padding>

		<center>
			<TableView fx:id="tvTable">
				<columns>
					<TableColumn id="pDate" />
					<TableColumn id="pScheme" />
					<TableColumn id="pSignal" />
					<TableColumn id="pValue" />
					<TableColumn id="pState" />
					<TableColumn id="pProcessTime" />
					<TableColumn id="pUser" />
				</columns>
			</TableView>
		</center>
	</BorderPane>
</Pane>